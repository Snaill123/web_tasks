<% @feedbacks.each do |feedback| %>
  <div class="blog-post">
    <h2 class="blog-post-title"><%= feedback.title %></h2>
    <p class="blog-post-meta"><%= feedback.created_at.strftime("%Y-%m-%d %H:%M")  %></p>
    <p class="blog-post-meta"><%= feedback.author  %></p>
    <p class="blog-post-meta"><%= feedback.email %></p>
    <%= feedback.content %>
  </div>
<% end %>

